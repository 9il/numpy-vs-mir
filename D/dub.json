{
  "name": "multid",
  "dflags-ldc": ["-mcpu=native"],
  "mainSourceFile" : "source/app.d",
  "dependencies": {
    "mir-algorithm": "~>3.9.6",
    "mir-random": "~>2.2.14",
    "numir": "~>2.0.5",
    "pretty_array": "~>1.0.2"
  },
  "configurations": [
    {
      "name": "multid",
      "targetType": "executable",
